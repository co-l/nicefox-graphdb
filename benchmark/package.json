{
  "name": "leangraph-benchmark",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "benchmark": "node --import tsx src/run.ts",
    "benchmark:quick": "node --import tsx src/run.ts --scale quick",
    "benchmark:full": "node --import tsx src/run.ts --scale full",
    "report": "node --import tsx src/report.ts",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:reset": "docker compose down -v && docker compose up -d"
  },
  "dependencies": {
    "leangraph": "file:..",
    "neo4j-driver": "^5.27.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0"
  }
}
